const readlineSync = require('readline-sync');

var losowa = Math.floor(Math.random()*100+1)
do{

    var a = readlineSync.questionInt();
    if(a != losowa){
        if(a<losowa) {console.log("Podałeś za małą wartość")}
        if(a>losowa) {console.log("Podałeś za dużą wartość")}
    }
}
while (a != losowa) {
    console.log("Gratulacje")
}
